{
  "name": "reajure",
  "version": "0.3.8",
  "main": "dist/commonjs/index.js",
  "module": "dist/module/index.js",
  "react-native": "src/index.ts",
  "types": "dist/typescript/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "keywords": [
    "react",
    "react-native"
  ],
  "scripts": {
    "watch": "tsc -b -w tsconfig.dev.json",
    "storybook": "start-storybook",
    "prepare": "del dist && bob build",
    "test": "jest",
    "test.watch": "jest --watch --coverage=false --changedSince=origin/master",
    "test.debug": "jest -o --watch --coverage=false"
  },
  "jest": {
    "preset": "@testing-library/react-native",
    "setupFiles": [
      "jest-expo/jest-preset.js",
      "@testing-library/react-native/jest-preset"
    ],
    "testRegex": "/test/.*\\.spec.ts$"
  },
  "@react-native-community/bob": {
    "source": "src",
    "output": "dist",
    "targets": [
      "commonjs",
      "module",
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  },
  "peerDependencies": {
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-native": "^0.61.5",
    "react-native-gesture-handler": "^1.5.3",
    "react-native-web": "0.0.0-26873b469"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@react-native-community/bob": "^0.8.0",
    "@storybook/addon-info": "^5.3.18",
    "@storybook/react": "^5.3.18",
    "@testing-library/react-hooks": "^3.3.0",
    "@types/jest": "^25.2.1",
    "@types/react": "^16.9.34",
    "@types/react-native": "^0.62.4",
    "babel-loader": "^8.1.0",
    "babel-preset-expo": "^8.1.0",
    "del-cli": "^3.0.0",
    "jest-expo": "38.0.2",
    "react": "^16.9.0",
    "react-docgen-typescript-loader": "^3.7.2",
    "react-dom": "^16.9.0",
    "react-native": "^0.61.5",
    "react-native-gesture-handler": "^1.5.3",
    "react-native-web": "0.0.0-26873b469",
    "react-test-renderer": "^16.13.1",
    "ts-loader": "^7.0.1",
    "typescript": "*",
    "typescript-loader": "^1.1.3"
  },
  "dependencies": {
    "@testing-library/react-native": "^5.0.3"
  }
}
